						<tbody>
							<tr>
								<th>Service</th>
								<th>Type</th>
								<th>Complexity Value</th>
							</tr>
							<xsl:for-each select="//sensor[definition/@sensorName='CodeLines']/results[starts-with(@name,'complexityFactor')]">
								<xsl:sort select="@value" data-type="number" order="descending"/>								
